{
  "version": 2,
  "name": "recipe-website",
  "alias": ["juliangilliatt.com", "www.juliangilliatt.com"],
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_SITE_URL": "https://juliangilliatt.com",
    "NEXT_PUBLIC_SITE_NAME": "Julian Gilliatt"
  },
  "redirects": [
    {
      "source": "/recipe/:slug",
      "destination": "/recipes/:slug",
      "permanent": true
    },
    {
      "source": "/post/:slug",
      "destination": "/blog/:slug",
      "permanent": true
    }
  ]
}